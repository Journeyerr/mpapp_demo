@import '../../common/common.wxss';

.form-box{
    padding-left: 30rpx;
}
.form-box input{
    height: 90rpx;
    border-bottom: 1rpx solid #ededed;
    color: #999;
}
.form-box .input-placeholder{
    color: #aaa;
}
.form-box button{
    margin:30rpx 30rpx 0 0;
}

.list {
    color: #777;
    font-size: 30rpx;
    height: auto;
    line-height: 40rpx;
    border-bottom: 1rpx solid #e9e9e9;
    /*text-align: center;*/
}

.list text{
    margin-left: 20px;
    margin-bottom: 10px;
}

.list-name{
    display: inline-block;
    margin: 12px;
    color: #555;
}

.list-address{
    display: block;
    font-size: 26rpx;
}
